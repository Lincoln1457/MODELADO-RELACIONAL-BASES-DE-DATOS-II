CREATE TABLE usuarui (
    id_usuario INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre varchar(300) NOT NULL,
    edad INT NOT NULL,
    email varchar(500)
);


INSERT INTO usuarui value(
